{
    "name": "bogddan/redirector",
    "description": "A comprehensive Laravel package for managing redirects with ease.",
    "type": "library",
    "keywords": [
        "laravel",
        "redirect",
        "manager",
        "url",
        "slug",
        "seo"
    ],
    "homepage": "https://github.com/bogddan/redirector",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Andrei Badea",
            "email": "zbiller@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Adrien Foulon",
            "email": "adrien.foulon@tukan.hu",
            "role": "Developer"
        },
        {
            "name": "Bogdan Ciobanu",
            "email": "bogddan@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^10.0",
        "illuminate/support": "^10.0",
        "illuminate/database": "^10.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "orchestra/testbench": "^8.5",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Bogddan\\Redirects\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bogddan\\Redirects\\Tests\\": "tests"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bogddan\\Redirects\\ServiceProvider"
            ]
        }
    }
}
